			"\\$http({",
			"\turl: '${url}',",
			"\tmethod: '${METHOD}'",
			"}).then(function (response) {",
			"\t${0}",
			"\tdeferred.resolve(response);",
			"}, function (error) {",
			"\tdeferred.reject(error);",
			"});",
			"return deferred.promise;",
			""
