	"Angular Service return a $$api.request promise": {
		"prefix": "apiRequest",
		"description": "Angular 1.x Service $$api.request return a promise\n",
		"body": [
			"function ${requestResource}(${config}) {",
			"\treturn \\$\\$api.request({",
			"\tmethod: '${GET}',",
			"\turl: '${url}',",
			"\tparams: ${config}",
			"});"
